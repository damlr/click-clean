//methode pour attribuer une note pour un utilisateur
public void attribuerNote(Utilisateur utilisateur) {
		this.add(utilisateur) ;
}

//methode pour laiser un commentaire d'utilisateur
public void commenter(Utilisateur utilisateur) {
		this.commenter(utilisateur) ;
}

//methode pour creer un litige d'utilisateur
public void creerLitige(Utilisateur utilisateur) {
		this.creerLitige(utilisateur) ;
}

//methode pour calculer la moyenne de notes d'un utilisateur
public void calculerMoyenne(Utilisateur utilisateur) {
		this.calculerMoyenne(utilisateur) ;
}
