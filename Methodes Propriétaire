//methode pour publier un annonce par un proprietaire
public void publierAnnonce(Proprietaire proprietaire) {
		this.publierAnnonce(proprietaire) ;
}

//methode pour enregistrer des proprietés d'un proprietaire
public void enregistrerProprietes(Proprietaire proprietaire) {
		this.enregistrerProprietes(proprietaire) ;
}

//methode pour supprimer des proprietés d'un proprietaire
public void supprimerProprietes(Proprietaire proprietaire) {
		this.supprimerProprietes(proprietaire) ;
}

//methode pour annuler l'annonce d'un proprietaire
public void annulerMissionCleaner(Proprietaire proprietaire) {
		this.annulerMissionProprietaire(proprietaire) ;
}

//methode pour definir un statut d'une mission par un proprietaire
public void definirStatut(Proprietaire proprietaire) {
		this.definirStatut(proprietaire) ;
}
