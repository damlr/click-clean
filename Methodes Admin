//methode pour supprimer un commentaire pour un admin
public void supprimerComm(Admin admin) {
		this.add(admin) ;
}

//methode pour valider un profile d'utilisateur par un admin
public void validerProfile(Admin admin) {
		this.validerProfile(admin) ;
}

//methode pour gerer les litiges d'utilisateurs par un admin
public void gererLitiges(Admin admin) {
		this.gererLitiges(admin) ;
}

//methode pour recevoir les litiges d'utilisateurs par un admin
public void recevoirLitiges(Admin admin) {
		this.recevoirLitiges(admin) ;
}

//methode pour ajouter des utilisateurs par un admin
public void ajouterUtilisateur(Admin admin) {
		this.ajouterUtilisateur(admin) ;
}
